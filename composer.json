{
    "name": "habityzer/kinde-bundle",
    "description": "Symfony bundle for Kinde authentication integration with JWT validation, webhooks, and user sync",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "kinde", "authentication", "oauth", "jwt", "webhook"],
    "authors": [
        {
            "name": "Vazgen Manukyan",
            "email": "vazgen.manukyan@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^6.4|^7.0",
        "symfony/security-bundle": "^6.4|^7.0",
        "symfony/http-client": "^6.4|^7.0",
        "symfony/event-dispatcher": "^6.4|^7.0",
        "symfony/cache": "^6.4|^7.0",
        "kinde-oss/kinde-auth-php": "^2.3",
        "firebase/php-jwt": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/phpunit-bridge": "^6.4|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Habityzer\\KindeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Habityzer\\KindeBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.4|^7.0"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

